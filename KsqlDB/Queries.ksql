	"Fish_Id" VARCHAR KEY,
	"Species" VARCHAR,
	"Height" DOUBLE,
	"Length" DOUBLE,
	"Timestamp" VARCHAR,
	"Prediction" STRUCT<"Weight" DOUBLE, "ModelTime" VARCHAR>
	)
WITH(KAFKA_TOPIC = 'weight-prediction', VALUE_FORMAT = 'JSON');


